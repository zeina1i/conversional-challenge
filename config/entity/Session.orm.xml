<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Entity\Session" table="sessions">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <many-to-one field="user" target-entity="App\Entity\User">
            <join-column name="user_id" referenced-column-name="id" />
        </many-to-one>
        <field name="appointmentTime" column="appointment_time" type="datetime" />
        <field name="activationTime" column="activation_time" type="datetime" />
    </entity>
</doctrine-mapping>